{
	"name": "Stop Autoplay for Youtube\u2122 Extended",
	"version": "4.1.0",
	"manifest_version": 2,

	"description": "Stops autoplay on YouTube\u2122. Extended: Starts the video when you focus the tab. Don't like? Use the version without 'Extended'.",
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"48": "icon48.png"
	},

	"author": "Jacob „kurtextrem\" Groß",
	"content_scripts": [{
		"matches": ["http://www.youtube.com/*", "http://youtube.com/*", "https://www.youtube.com/*", "https://youtube.com/*"],
		"js": ["start.js"],
		"run_at": "document_start"
	}],

	"web_accessible_resources": ["main.js"],

	"options_page": "options.html",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},

	"minimum_chrome_version": "42"
}